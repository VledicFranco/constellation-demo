# Lambda Map-Reduce — Transform and aggregate
# Demonstrates: map with lambda, list-sum for reduction

use stdlib.collection
use stdlib.math
use stdlib.list

in numbers: List<Int>

# Map: double each number
doubled = map(numbers, (x) => multiply(x, 2))

# Map: negate each
negated = map(numbers, (x) => negate(x))

# Reduce: sum the doubled values
total = list-sum(doubled)

out doubled
out negated
out total
