# Constellation Demo - Default Environment Variables

# Server
CONSTELLATION_PORT=8080
CONSTELLATION_GRPC_PORT=9090
CONSTELLATION_SCHEDULER_ENABLED=true
CONSTELLATION_SCHEDULER_MAX_CONCURRENCY=16
CONSTELLATION_SCHEDULER_STARVATION_TIMEOUT=30s

# Memcached
MEMCACHED_ADDRESS=memcached:11211

# TypeScript Provider
TS_EXECUTOR_PORT=50051
TS_EXECUTOR_HOST=provider-ts

# Scala Provider
SCALA_EXECUTOR_PORT=50052
SCALA_EXECUTOR_HOST=provider-scala

# Grafana
GF_SECURITY_ADMIN_PASSWORD=demo
GF_AUTH_ANONYMOUS_ENABLED=true
